{
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf",
    "singleAttributePerLine": true,
    "plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-classnames"]
}
